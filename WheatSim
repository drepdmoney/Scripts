if game.PlaceID == 10106105124 then

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local wfsWindow = Library.CreateLib("Dreps Hub | Wheat Farming Sim", "DarkTheme")

local wfsmainTab = wfsWindow:NewTab("Main")
local wfscodeSection = wfsmainTab:NewSection("Main")
wfscodeSection:NewLabel("One redeem code, script will auto equip pet from it.")

wfscodeSection:NewToggle("Dupe Lava Beast", "Found out dupe, basically infinite boost.", function(state)
    if state then
        getgenv().autoDupe = true
        spawn(function()
            while autoDupe do task.wait(0.5)
                local args = {
                    [1] = "tamepets"
                }
                
                game:GetService("ReplicatedStorage").RemoteFunctions.Redeem:InvokeServer(unpack(args))
                wait()
                local args = {
                    [1] = "lavabeast"
                }
                
                game:GetService("ReplicatedStorage").RemoteFunctions.Redeem:InvokeServer(unpack(args))
                wait()
                local args = {
                    [1] = "pets"
                }
                
                game:GetService("ReplicatedStorage").RemoteFunctions.Redeem:InvokeServer(unpack(args))   
                wait()
                local args = {
                    [1] = "taming"
                }
                
                game:GetService("ReplicatedStorage").RemoteFunctions.Redeem:InvokeServer(unpack(args))
                wait()
                -- Script generated by SimpleSpy - credits to exx#9394
                
                local args = {
                    [1] = "megarebirth"
                }
                
                game:GetService("ReplicatedStorage").RemoteFunctions.Redeem:InvokeServer(unpack(args))
            wait(0.1)
            -- Script generated by SimpleSpy - credits to exx#9394
            
            local args = {
                [1] = {
                    ["equip"] = true,
                    ["stat"] = "Pet",
                    ["id"] = 170
                }
            }
            
            game:GetService("ReplicatedStorage").RemoteFunctions.Equip:InvokeServer(unpack(args))
        end
    end)
        else
        getgenv().autoDupe = false
    end
end)

wfscodeSection:NewButton("Redeem all codes", " ", function()
    local args = {
        [1] = "tamepets"
    }
    
    game:GetService("ReplicatedStorage").RemoteFunctions.Redeem:InvokeServer(unpack(args))
    wait()
    local args = {
