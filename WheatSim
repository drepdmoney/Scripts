wfswheatSection:NewToggle("Auto Sell Wheat", "Can enable with wheat farm", function(state)
    if state then
        getgenv().tpsell = true
        spawn(function()
            while tpsell do task.wait(0.1)
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(121, 46, -419)
                wait()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(103, 46, -427)
            end

        end)
    else
        getgenv().tpsell = false
    
    end
end)
