wait(0.1)
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = {
        ["equip"] = true,
        ["stat"] = "Pet",
        ["id"] = 170
    }
}

game:GetService("ReplicatedStorage").RemoteFunctions.Equip:InvokeServer(unpack(args))


end)



local wfsplayerSection = wfsmainTab:NewSection("Player")

wfsplayerSection:NewSlider("WalkSpeed", "Change walkspeed", 350, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

wfsplayerSection:NewButton("Reset Walkspeed", "", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)

wfsplayerSection:NewButton("Teleport to Sell", "", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(103, 46, -427)
end)

wfsplayerSection:NewButton("Teleport to Shop", "", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(234, 46, -457)
end)
wfsplayerSection:NewButton("Teleport to Codes", "", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(409, 46, -393)
end)

local wfsfarmTab = wfsWindow:NewTab("Farm")

local wfswheatSection = wfsfarmTab:NewSection("Wheat Farm")


wfswheatSection:NewToggle("Auto Wheat Farm", "", function(state)
    if state then
        wait(0.1)
        game.StarterGui:SetCore("SendNotification", {
        Title = "READ ME"; -- the title (ofc)
        Text = "If you turn this off, it will take a second or few to finish what its doing"; -- what the text says (ofc)
        
        Duration = 5; -- how long the notification should in secounds
        })
        getgenv().wheatfarm = true
    spawn (function()
    while wheatfarm do task.wait()
    local teleport_table = {
        location1 = Vector3.new(645.389, 46.596, 392.875) -- location
    }
    
    local tween_s = game:GetService('TweenService')
    local tweeninfo = TweenInfo.new(1.5, Enum.EasingStyle.Linear) -- replace 1 with how many secs it takes to get there
    
    local lp =  game.Players.LocalPlayer
    
    function bypass_teleport(v)
        if lp.Character and   
        lp.Character:FindFirstChild('HumanoidRootPart') then    
    
            local cf = CFrame.new(v)
            local a = tween_s:Create(lp.Character.HumanoidRootPart, tweeninfo,{CFrame=cf})
    
             a:Play()
        end
    end 
    
    bypass_teleport(teleport_table.location1)
wait(1)
local teleport_table = {
    location1 = Vector3.new(645.363, 46.596, -353.35) -- location
}

local tween_s = game:GetService('TweenService')
local tweeninfo = TweenInfo.new(1.5, Enum.EasingStyle.Linear) -- replace 1 with how many secs it takes to get there

local lp =  game.Players.LocalPlayer

function bypass_teleport(v)
    if lp.Character and   
    lp.Character:FindFirstChild('HumanoidRootPart') then    

        local cf = CFrame.new(v)
        local a = tween_s:Create(lp.Character.HumanoidRootPart, tweeninfo,{CFrame=cf})

         a:Play()
    end
end 

bypass_teleport(teleport_table.location1)
wait(1)
