local clickMod = require(game:GetService("Players").LocalPlayer.PlayerScripts.Aero.Controllers.UI.Click)

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

    local cmWindow = Library.CreateLib("Dreps Hub | Clicking Madness", "DarkTheme")

    local cmMainTab = cmWindow:NewTab("Main")

    local cmCodeSection = cmMainTab:NewSection("Codes")

    cmCodeSection:NewButton("Redeem all codes", " ", function()
        local args = {
            [1] = "Russo" -- replace with code
        }
        
        game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem:InvokeServer(unpack(args))
        wait()
        local args = {
            [1] = "30KLIKES" -- replace with code
        }
        
        game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem:InvokeServer(unpack(args))
        wait()
        local args = {
            [1] = "10KLIKES" -- replace with code
        }
        
        game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem:InvokeServer(unpack(args))
        wait()
        local args = {
            [1] = "Release" -- replace with code
        }
        
        game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem:InvokeServer(unpack(args))
        wait()
        local args = {
            [1] = "ELSA" -- replace with code
        }
        
        game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem:InvokeServer(unpack(args))
        wait()
        local args = {
            [1] = "Test" -- replace with code
        }
        
        game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.CodeService.Redeem:InvokeServer(unpack(args))
    end)


    local cmPlayerSection = cmMainTab:NewSection("Player")

cmPlayerSection:NewSlider("WalkSpeed", "Change walkspeed", 350, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

cmPlayerSection:NewButton("Reset Walkspeed", "", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)


local cmFarmTab = cmWindow:NewTab("Farm")

local cmFarmSection = cmFarmTab:NewSection("Auto Farm")


cmFarmSection:NewToggle("Auto Click", "Self explanatory.", function(state)
    if state then
      getgenv().autoclick = true
      spawn(function()
        while autoclick do task.wait()
        clickMod:Click()
        end
    end)
    else
        getgenv().autoclick = false
    end
end)

cmFarmSection:NewToggle("Auto Rebirth", " ", function(state)
    if state then
getgenv().autoRebirth = true
spawn(function()
    while autoRebirth do task.wait()
        local args = {
    [1] = 10
}

game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.RebirthService.BuyRebirths:FireServer(unpack(args))
end
end)
    else
       getgenv().autoRebirth = false
    end
end)
local cmBossSection = cmFarmTab:NewSection("Boss Farm")
cmBossSection:NewLabel("Recommend enable with auto rebirth.")
cmBossSection:NewToggle("Auto Farm Event Boss", "Farm Current Event Boss", function(state)
    if state then
        getgenv().teleportBoss = true
        spawn(function()
            while teleportBoss do task.wait()
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(790, 10, -265)
            end

        end)
    wait(0.1)
    getgenv().bossClick = true
    spawn(function()
        while bossClick do task.wait()
            clickMod:Click()
        end

    end)
    else
       getgenv().teleportBoss = false
        getgenv().bossClick = false 
    end
end)

local cmTeleportTab = cmWindow:NewTab("Teleports")

local cmTeleportSection = cmTeleportTab:NewSection("Teleports | Scroll down")
